<h1>Listing deal_milestones</h1>

<table>
  <tr>
    <th>Color</th>
    <th>Sort order</th>
    <th>Title</th>
    <th>Description</th>
    <th>Probability</th>
    <th>Status</th>
    <th>Tenant</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @deal_milestones.each do |deal_milestone| %>
  <tr>
    <td><%= deal_milestone.color %></td>
    <td><%= deal_milestone.sort_order %></td>
    <td><%= deal_milestone.title %></td>
    <td><%= deal_milestone.description %></td>
    <td><%= deal_milestone.probability %></td>
    <td><%= deal_milestone.status %></td>
    <td><%= deal_milestone.tenant %></td>
    <td><%= link_to 'Show', deal_milestone %></td>
    <td><%= link_to 'Edit', edit_deal_milestone_path(deal_milestone) %></td>
    <td><%= link_to 'Destroy', deal_milestone, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Deal milestone', new_deal_milestone_path %>
