<h1>Listing field_values</h1>

<table>
  <tr>
    <th>Value</th>
    <th>Entity</th>
    <th>Tenant</th>
    <th>Field</th>
    <th>Entity type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @field_values.each do |field_value| %>
  <tr>
    <td><%= field_value.value %></td>
    <td><%= field_value.entity %></td>
    <td><%= field_value.tenant %></td>
    <td><%= field_value.field %></td>
    <td><%= field_value.entity_type %></td>
    <td><%= link_to 'Show', field_value %></td>
    <td><%= link_to 'Edit', edit_field_value_path(field_value) %></td>
    <td><%= link_to 'Destroy', field_value, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Field value', new_field_value_path %>
